      {{/if}}

      {{#if report.graduate_percentage}}
        $("#graduate_percentage").val("{{report.graduate_percentage}}");
      {{/if}}

      {{#if report.benefits}}
        $("#benefits_{{report.benefits.option}}").attr("checked", true);
        if ($("#benefits_si").is(":checked"))
          $("#benefits_cuales").val("{{report.benefits.names}}").attr("required", true).removeAttr("disabled");
      {{/if}}

      {{#if report.programs}}
        $("#program_quantity").val("{{report.programs.length}}").trigger("change");
        {{#each report.programs as |obj index|}}
          {{#if obj.level}}
            $("#program"+({{index}}+1)+"_level_{{obj.level}}").attr("checked", true);
          {{/if}}
          {{#if obj.name}}
            $("#program"+({{index}}+1)+"_name").val("{{obj.name}}");
          {{/if}}
          {{#if obj.beneficiaries}}
            $("#program"+({{index}}+1)+"_beneficiaries").val("{{obj.beneficiaries}}");
          {{/if}}
          {{#if obj.term}}
            $("#program"+({{index}}+1)+"_term").val("{{obj.term}}");
          {{/if}}
          {{#if obj.investment}}
            $("#program"+({{index}}+1)+"_investment").val("{{obj.investment}}");
          {{/if}}
          {{#if obj.rate}}
            $("#program"+({{index}}+1)+"_rate_{{obj.rate.type}}").attr("checked", true);
            $("#program"+({{index}}+1)+"_rate_percentage").val("{{obj.rate.percentage}}").attr("required", true).removeAttr("disabled");
          {{/if}}
          {{#if obj.repayment}}
            $("#program"+({{index}}+1)+"_repayment_{{obj.repayment.type}}").attr("checked", true);
            if ($("#program"+({{index}}+1)+"_repayment_otra").is(":checked"))
              $("#program"+({{index}}+1)+"_repayment_other").val("{{obj.repayment.other}}").attr("required", true).removeAttr("disabled");
          {{/if}}
          {{#if obj.grace}}
            $("#program"+({{index}}+1)+"_grace").val("{{obj.grace}}");
          {{/if}}
          {{#if obj.warranties}}
            {{#each obj.warranties as |warranty|}}
              $("#program"+({{index}}+1)+"_warranties_{{warranty}}").attr("checked", true);
            {{/each}}
            if ($("#program"+({{index}}+1)+"_warranties_otra").is(":checked"))
              $("#program"+({{index}}+1)+"_warranties_other").val("{{obj.warranties_other}}").attr("required", true).removeAttr("disabled");
          {{/if}}
          {{#if obj.guarantors}}
            {{#each obj.guarantors as |guarantor|}}
              $("#program"+({{index}}+1)+"_guarantors_{{guarantor}}").attr("checked", true);
            {{/each}}
            if ($("#program"+({{index}}+1)+"_guarantors_otra").is(":checked"))
              $("#program"+({{index}}+1)+"_guarantors_other").val("{{obj.guarantors_other}}").attr("required", true).removeAttr("disabled");
          {{/if}}
          {{#if obj.credit_pais}}
            $("#program"+({{index}}+1)+"_credit_pais_new").val("{{obj.credit_pais.new}}");
            $("#program"+({{index}}+1)+"_credit_pais_old").val("{{obj.credit_pais.old}}");
          {{/if}}
          {{#if obj.credit_exterior}}
            $("#program"+({{index}}+1)+"_credit_exterior_new").val("{{obj.credit_exterior.new}}");
            $("#program"+({{index}}+1)+"_credit_exterior_old").val("{{obj.credit_exterior.old}}");
          {{/if}}
        {{/each}}
      {{/if}}

      // $("#tooltip").attr('data-tip', );
      // console.log($("#tooltip").attr('data-tip'));
    });
  </script>
	<script type="text/javascript">
    function changePanelsQuantity(value) {
      // console.log(value);
      let container = $("#programs_container");

      let current_quantity = container.children().length;
      // console.log(current_quantity);
      let target_quantity = value - current_quantity;
      if (target_quantity > 0) {
        for (let i = current_quantity + 1; i <= value; i++) {
          var html = `
            <div class="x_panel">
              <div class="x_title">
                <h2>Programa ${i} </h2>
                <div class="nav navbar-right">
                  <a class="collapse-link pointer">
                    <i class="fa fa-chevron-up"></i>
                  </a>
                </div>
                <div class="clearfix"></div>
              </div>
              <div class="x_content">
                <div class="form-group">
                  <label class="control-label col-md-3 col-sm-3 col-xs-12">
                    Nivel
                  </label>
                  <div class="col-md-9 col-sm-9 col-xs-12">
                    <div class="radio">
                      <label>
                        <input class="level" type="radio" value="pregrado" id="${'program'+i+'_level_pregrado'}" name="${'program'+i+'_level'}" checked required>
                        Pregrado
                      </label>
                    </div>
                    <div class="radio">
                      <label>
                        <input class="level" type="radio" value="posgrado" id="${'program'+i+'_level_posgrado'}" name="${'program'+i+'_level'}">
                        Posgrado
                      </label>
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <label class="control-label col-md-3 col-sm-3 col-xs-12" for="${'program'+i+'_name'}">
                    1. Nombre del Programa o Línea de C.E.
                  </label>
                  <div class="col-md-6 col-sm-6 col-xs-12">
                    <input type="text" id="${'program'+i+'_name'}" name="${'program'+i+'_name'}" class="form-control" required>
                  </div>
                </div>
                <div class="form-group">
                  <label class="control-label col-md-3 col-sm-3 col-xs-12" for="${'program'+i+'_beneficiaries'}">
                    2. Número de beneficiarios por año
                  </label>
                  <div class="col-md-6 col-sm-6 col-xs-12">
                    <input type="number" id="${'program'+i+'_beneficiaries'}" name="${'program'+i+'_beneficiaries'}" class="mt-1"
                           min="0" step="1">
                  </div>
                </div>
                <div class="form-group">
                  <label class="control-label col-md-3 col-sm-3 col-xs-12" for="${'program'+i+'_term'}">
                    3. Plazo (en años)
                  </label>
                  <div class="col-md-6 col-sm-6 col-xs-12">
                    <input type="number" id="${'program'+i+'_term'}" name="${'program'+i+'_term'}" class="mt-1"
                           min="1" step="1">
                  </div>
                </div>
                <div class="form-group">
                  <label class="control-label col-md-3 col-sm-3 col-xs-12" for="${'program'+i+'_investment'}">
                    4. Monto total de recursos invertidos por año (USD al cierre del año fiscal)
                  </label>
                  <div class="col-md-6 col-sm-6 col-xs-12">
                    <input type="number" id="${'program'+i+'_investment'}" name="${'program'+i+'_investment'}" class="mt-2"
                           min="0">
                  </div>
                </div>
                <div class="form-group">
                  <label class="col-md-3 col-sm-3 col-xs-12 control-label">
                    5. Tasa de interés (anual)
                  </label>
                  <div class="col-md-9 col-sm-9 col-xs-12">
                    <div class="radio">
                      <label>
                        <input class="rate" type="radio" value="fija" id="${'program'+i+'_rate_fija'}" name="${'program'+i+'_rate'}">
                        Fija
                      </label>
                    </div>
                    <div class="radio">
                      <label>
                        <input class="rate" type="radio" value="variable" id="${'program'+i+'_rate_variable'}" name="${'program'+i+'_rate'}">
                        Variable
                      </label>
                    </div>
                    <label id="${'program'+i+'_rate_label'}" for="${'program'+i+'_rate_percentage'}">Porcentaje anual: </label>
                    <input type="number" id="${'program'+i+'_rate_percentage'}" name="${'program'+i+'_rate_percentage'}" min="0" disabled> %
                  </div>
                </div>
                <div class="form-group">
                  <label class="col-md-3 col-sm-3 col-xs-12 control-label">
                    6. Tipo de repago
                    <span id="tooltip" class="tool" data-tip="Se refiere a la forma de recuperación del crédito"
                                tabindex="1">
                      <i class="fa fa-question-circle"></i>
                    </span>
                  </label>
                  <div class="col-md-9 col-sm-9 col-xs-12">
                    <div class="radio">
                      <label>
                        <input class="repayment" type="radio" value="fijo" id="${'program'+i+'_repayment_fijo'}" name="${'program'+i+'_repayment'}">
                        Fijo
                      </label>
                    </div>
                    <div class="radio">
                      <label>
                        <input class="repayment" type="radio" value="creciente" id="${'program'+i+'_repayment_creciente'}" name="${'program'+i+'_repayment'}">
                        Creciente
                      </label>
                    </div>
                    <div class="radio">
                      <label>
                        <input class="repayment" type="radio" value="salario" id="${'program'+i+'_repayment_salario'}" name="${'program'+i+'_repayment'}">
                        En función del salario
                      </label>
                    </div>
                    <div class="radio">
                      <label>
                        <input class="repayment" type="radio" value="otra" id="${'program'+i+'_repayment_otra'}" name="${'program'+i+'_repayment'}">
                        Otra
                      </label>
                      <label for="${'program'+i+'_repayment_other'}"></label>
                      <input type="text" disabled id="${'program'+i+'_repayment_other'}" name="${'program'+i+'_repayment_other'}">
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <label class="control-label col-md-3 col-sm-3 col-xs-12" for="${'program'+i+'_grace'}">
                    7. Periodo de gracia (años)
                    <span id="tooltip" class="tool" data-tip="Se refiere al tiempo que tiene el beneficiario entre la finalización del desembolso del crédito y comenzar el repago."
                                tabindex="1">
                      <i class="fa fa-question-circle"></i>
                    </span>
                  </label>
                  <div class="col-md-6 col-sm-6 col-xs-12">
                    <input type="number" id="${'program'+i+'_grace'}" class="mt-1" min="0" step="0.1">
                  </div>
                </div>
                <div class="form-group">
                  <label class="col-md-3 col-sm-3 col-xs-12 control-label">
                    8. Tipo de garantías
                  </label>
                  <div class="col-md-6 col-sm-6 col-xs-12">
                    <div class="checkbox">
                      <label>
                        <input class="warranties" type="checkbox" value="personal" id="${'program'+i+'_warranties_personal'}" name="${'program'+i+'_warranties'}">
                        Personal (codeudor, obligado solidario)
                      </label>
                    </div>
                    <div class="checkbox">
                      <label>
                        <input class="warranties" type="checkbox" value="real" id="${'program'+i+'_warranties_real'}" name="${'program'+i+'_warranties'}">
                        Real (pagaré, hipoteca)
                      </label>
                    </div>
                    <div class="checkbox">
                      <label>
                        <input class="warranties" type="checkbox" value="mixta" id="${'program'+i+'_warranties_mixta'}" name="${'program'+i+'_warranties'}">
                        Mixta
                      </label>
                    </div>
                    <div class="checkbox">
                      <label>
                        <input class="warranties" type="checkbox" value="seguro" id="${'program'+i+'_warranties_seguro'}" name="${'program'+i+'_warranties'}">
                        Seguro de vida
                      </label>
                    </div>
                    <div class="checkbox">
                      <label>
                        <input class="warranties" type="checkbox" value="no" id="${'program'+i+'_warranties_no'}" name="${'program'+i+'_warranties'}">
                        No tiene
                      </label>
                    </div>
                    <div class="checkbox">
                      <label>
                        <input class="warranties" type="checkbox" value="otra" id="${'program'+i+'_warranties_otra'}" name="${'program'+i+'_warranties'}">
                        Otra
                      </label>
                      <label for="${'program'+i+'_warranties_other'}"></label>
                      <input type="text" disabled id="${'program'+i+'_warranties_other'}" name="${'program'+i+'_warranties_other'}">
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <label class="col-md-3 col-sm-3 col-xs-12 control-label">
                    9. Tipo de codeudor o fiador
                  </label>
                  <div class="col-md-6 col-sm-6 col-xs-12">
                    <div class="checkbox">
                      <label>
                        <input class="guarantors" type="checkbox" value="estudiante" id="${'program'+i+'_guarantors_estudiante'}" name="${'program'+i+'_guarantors'}">
                        El mismo estudiante
                      </label>
                    </div>
                    <div class="checkbox">
                      <label>
                        <input class="guarantors" type="checkbox" value="padre" id="${'program'+i+'_guarantors_padre'}" name="${'program'+i+'_guarantors'}">
                        Padre de familia
                      </label>
                    </div>
                    <div class="checkbox">
                      <label>
                        <input class="guarantors" type="checkbox" value="otra" id="${'program'+i+'_guarantors_otra'}" name="${'program'+i+'_guarantors'}">
                        Otra
                      </label>
                      <label for="${'program'+i+'_guarantors_other'}"></label>
                      <input type="text" disabled id="${'program'+i+'_guarantors_other'}" name="${'program'+i+'_guarantors_other'}">
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <label class="col-md-3 col-sm-3 col-xs-12 control-label">
                    10. Número de créditos otorgados por año en el País
                  </label>
                  <div class="col-md-6 col-sm-6 col-xs-12 pt">
                    <div>
                      <label for="${'program'+i+'_credit_pais_new'}">Créditos nuevos: </label>
                      <input type="number" id="${'program'+i+'_credit_pais_new'}" name="${'program'+i+'_credit_pais_new'}" min="0">
                    </div>
                    <div>
                      <label for="${'program'+i+'_credit_pais_old'}">Créditos antiguos: </label>
                      <input type="number" id="${'program'+i+'_credit_pais_old'}" name="${'program'+i+'_credit_pais_old'}" min="0">
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <label class="col-md-3 col-sm-3 col-xs-12 control-label">
                    10A. Número de créditos otorgados por año en el Exterior
                  </label>
                  <div class="col-md-6 col-sm-6 col-xs-12 pt">
                    <div>
                      <label for="${'program'+i+'_credit_exterior_new'}">Créditos nuevos: </label>
                      <input type="number" id="${'program'+i+'_credit_exterior_new'}" name="${'program'+i+'_credit_exterior_new'}" min="0">
                    </div>
                    <div>
                      <label for="${'program'+i+'_credit_exterior_old'}">Créditos antiguos: </label>
                      <input type="number" id="${'program'+i+'_credit_exterior_old'}" name="${'program'+i+'_credit_exterior_old'}" min="0">
                    </div>
                  </div>
                </div>
              </div>
            </div>
          `;
          container.append(html);
        }
      } else {
        target_quantity *= -1;
        for (let i = 0; i < target_quantity; i++) {
          $("#programs_container .x_panel").last().remove();
        }
      }
    }

    function sendForReview() {
      $('<input />').attr('type', 'hidden')
          .attr('name', "complete")
          .attr('value', 1)
          .appendTo('#demo-form2');

      document.getElementById('demo-form2').submit();
    }
  </script>
  </body>
